module libspoofax/editor/completion/interface

// interface for collecting the suggestions
// getting the indentation, ws and separator
rules
  suggest-completions(|iset)           = fail
  suggest-completions-right-rec(|iset) = fail
  suggest-completions-left-rec(|iset)  = fail
  is-SDF-placeholder                   = fail
  is-left-recursive                    = fail
  is-right-recursive                   = fail
  is-lexical-completion-sort           = fail