<?xml version="1.0" encoding="UTF-8"?>
<configuration status="warn">
  <appenders>
    <console
      name="stdout"
      target="SYSTEM_OUT"
    >
      <patternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
    </console>
  </appenders>
  <loggers>
    <root level="debug">
      <appenderRef ref="stdout" />
    </root>
  </loggers>
</configuration>
